    "@types/node": "22.15.19",
    "@vicons/tabler": "0.13.0",
    "@vue/test-utils": "2.4.6",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
